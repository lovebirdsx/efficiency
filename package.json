{
  "name": "efficiency",
  "displayName": "Efficiency",
  "description": "Tools to improve your efficiency",
  "version": "1.0.3",
  "publisher": "lovebird",
  "icon": "images/efficiency.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/lovebirdsx/efficiency.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "efficiency.convertToEnglishPunctuation",
        "title": "Efficiency: Convert To English Punctuation"
      },
      {
        "command": "efficiency.convertToChinesePunctuation",
        "title": "Efficiency: Convert To Chinese Punctuation"
      },
      {
        "command": "efficiency.deleteCurrentFile",
        "title": "Efficiency: Delete Current File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "mocha": "mocha"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "chai": "^4.3.7",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  }
}
